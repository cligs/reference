          <list type="gloss">
            <label>TEMPS</label>
            <item>1640</item>
            <label>LIEU</label>
            <item>La salle de l'Hôtel de Bourgogne</item>
          </list>
        </set>
      </front>
    </egXML>
  </exemplum>
