        <encodingDesc>
          <unitDecl>
            <unitDef xml:id="merk" type="area">
              <label>merk</label>
              <placeName ref="#Scotland"/>
              <desc>A merk was an area of land determined variably by its agricultural productivity.</desc>
            </unitDef>
          </unitDecl>
        </encodingDesc>
      </egXML>
