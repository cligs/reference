        <valItem ident="candidate"/>
        <valItem ident="cleared"/>
        <valItem ident="deprecated"/>
        <valItem ident="draft"/>
        <valItem ident="embargoed"/>
        <valItem ident="expired"/>
        <valItem ident="frozen"/>
        <valItem ident="galley"/>
        <valItem ident="proposed"/>
        <valItem ident="published"/>
