                     <ref name="tei_model.global"/>
                  </zeroOrMore>
        
               </group>
            </oneOrMore>
            <zeroOrMore>
               <group>
        
                  <ref name="tei_model.divBottom"/>
        
        
                  <zeroOrMore>
                     <ref name="tei_model.global"/>
                  </zeroOrMore>
        
